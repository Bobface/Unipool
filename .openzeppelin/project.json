{
  "manifestVersion": "2.2",
  "contracts": {
    "iUniPool": "iUniPool"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.4.0"
  },
  "name": "Unipool",
  "version": "0.0.1",
  "telemetryOptIn": true,
  "compiler": {
    "manager": "openzeppelin",
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "500"
      }
    },
    "solcVersion": "0.5.13",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts",
    "typechain": {
      "enabled": false
    }
  }
}
